{
    "refresh_mills": 500,
    "handlers": [
        {
            "type": "Graph",
            "id": "graph",
            "regex": ".*number: (\\d+)",
            "params": {
                "name": "Numbers Graph",
                "width": 50,
                "height": 15
            }
        },
        {
            "type": "Numbers",
            "id": "max",
            "regex": ".*number: (\\d+)",
            "params": {
                "name": "Max Number",
                "method": "max"
            }
        },
        {
            "type": "Numbers",
            "id": "min",
            "regex": ".*number: (\\d+)",
            "params": {
                "name": "Min Number",
                "method": "min"
            }
        },
        {
            "type": "Numbers",
            "id": "avg",
            "regex": ".*number: (\\d+)",
            "params": {
                "name": "Avarage",
                "method": "avg"
            }
        },
        {
            "type": "Progress",
            "id": "progress",
            "regex": "progress: (\\d+)",
            "params": {
                "name": "Progress",
                "defaultTotal": 1000,
                "width": 100
            }
        },
        {
            "type": "Numbers",
            "id": "processed",
            "regex": "progress: (\\d+)",
            "params": {
                "name": "Processed",
                "method": "latest"
            }
        },
        {
            "type": "Numbers",
            "id": "total",
            "regex": "target: (\\d+)",
            "params": {
                "name": "Out of",
                "method": "latest"
            }
        },
        {
            "type": "Progress",
            "id": "usage",
            "regex": "resources usage: (\\d+)/\\d+",
            "params": {
                "name": "Resources Usage",
                "regexTotal": "resources usage: \\d+/(\\d+)",
                "width": 100
            }
        },
        {
            "type": "Numbers",
            "id": "used",
            "regex": "resources usage: (\\d+)/\\d+",
            "params": {
                "name": "Used",
                "method": "latest"
            }
        },
        {
            "type": "Numbers",
            "id": "total_res",
            "regex": "resources usage: \\d+/(\\d+)",
            "params": {
                "name": "Available",
                "method": "latest"
            }
        },
        {
            "type": "Numbers",
            "id": "errors",
            "regex": "error: .*",
            "params": {
                "name": "Errors",
                "method": "count",
                "thresholdMethod": "lt",
                "threshold": 50
            }
        },
        {
            "type": "Numbers",
            "id": "last_err",
            "regex": "error: Uh oh! Something bad happened. Code:(\\d+)",
            "params": {
                "name": "Last error code",
                "method": "latest"                
            }
        }
    ],
    "layouts": [
        {
            "type": "Pipe"
        },
        {            
            "type": "Line",
            "handlers": [
                {
                    "id": "graph"
                },
                {
                    "id": "max"
                },
                {
                    "id": "min"
                },
                {
                    "id": "avg"
                }
            ]
        },
        {
            "type": "Line",
            "handlers": [
                {
                    "id": "usage"
                },
                {
                    "id": "used"
                },
                {
                    "id": "total_res"
                }
            ]
        },        
        {
            "type": "Line",
            "handlers": [
                {
                    "id": "errors"
                },
                {
                    "id": "last_err"
                }
            ]
        },
        {
            "type": "Line",
            "handlers": [
                {
                    "id": "progress"
                },
                {
                    "id": "processed"
                },
                {
                    "id": "total"
                }
            ]
        }
    ]
}